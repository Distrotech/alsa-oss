#!/bin/sh

# A simple script to facilitate the use of the OSS compatibility library.
# Usage:
#	aoss <command> <command options and arguments>

if [ -d /proc/asound ]; then
  prefix=@prefix@
  libdir=@libdir@
  LD_PRELOAD=${libdir}/libaoss.so${LD_PRELOAD:+:$LD_PRELOAD} exec "$@"
else
  exec "$@"
fi
exit 1
